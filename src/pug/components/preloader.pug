.preloader
	picture
		source(srcset=`${baseDir}images/guy-on-rocket.webp, ${baseDir}images/guy-on-rocket@2x.webp 2x` type="image/webp")
		source(srcset=`${baseDir}images/guy-on-rocket.png, ${baseDir}images/guy-on-rocket@2x.png 2x` type="image/png")
		img.preloader__image(
			src=`${baseDir}images/guy-on-rocket.png`
			data-html2canvas-ignore
			alt="Preloader"
			width="591"
			height="591"
		)
